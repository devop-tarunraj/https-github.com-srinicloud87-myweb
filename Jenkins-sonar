pipeline{
    agent any
     tools {
        maven 'Maven3.6.3'
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'git-cred', url: 'https://github.com/devop-tarunraj/https-github.com-srinicloud87-myweb.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean sonar:sonar package"
                
            }
        }
    }
}

